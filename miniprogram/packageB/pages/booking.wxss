/* pages/myPage/booking.wxss */
.img {
  width: 50rpx;
  height: 50rpx;
  border: 0px solid red;
  margin-right: 9rpx;
  margin-bottom: -15rpx;
}

.swiper-tab {
  width: 100%;
  border-bottom: 2rpx solid rgb(151, 150, 150);
  text-align: center;
  line-height: 80rpx;
}

.swiper-tab-list {
  font-size: 30rpx;
  display: inline-block;
  width: 25%;
  color: #777;
}

.on {
  color: #da7c0c;
  border-bottom: 5rpx solid #da7c0c;
}

.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
}

.swiper-box view {
  text-align: center;
}

/*订单列表*/

.weui-cells {
  position: relative;
  margin: 15px;
  border-radius: 5px;
  padding-bottom: 6px !important;
  line-height: 1.41176471;
  font-size: 14px;
}


