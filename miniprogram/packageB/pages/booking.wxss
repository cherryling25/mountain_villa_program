.weui-cell {
  margin-left: 10px;
  margin-right: 10px;
  border-bottom: 1px solid rgba(0, 0, 0, 0.5);
  padding: 8px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.weui-cell__ft {
  text-align: right;
  color: #201e1e;
}

.last {
  padding-top: 6px;
  color: red;
}

.order-cells1 {
  position: relative;
  margin: 75px 15px;
}

.order-cells1 image {
  width: 120rpx;
  height: 120rpx;
}

.order-cells1 .text {
  color: rgba(0, 0, 0, 0.5);
}
