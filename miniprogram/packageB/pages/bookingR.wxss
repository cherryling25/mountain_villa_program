/* pages/myPage/booking.wxss */
.cardWrap{
  display: flex;
  justify-content: center;
  margin-top: 20rpx;
}
.card{
  background: white;
  width: 88%;
  padding: 20rpx;
  border-radius: 15rpx;
  border: 1px solid #8cbcfc;
}
.cardTitle{
  display: flex;
  justify-content: space-between;
  font-size: 33rpx;
}
.tip{
  color: #8cbcfc;
}
.time{
  margin-left: 100rpx;
}
.money{
  margin-left: 60rpx;
}
.price{
  font-size: 33rpx;
}
.cardTime{
  margin-top: 40rpx;
  margin-left: 30rpx;
  color: gray;
}
.cardContent{
  display: flex;
  font-size: 30rpx;
  margin-top: 30rpx;
}
.cardImg{
  background: #8cbcfc;
  display: inline-block;
  padding: 30rpx 0 30rpx 30rpx;
  border-radius: 10rpx;
}
.total, .btn{
  font-size: 30rpx;
  display: flex;
  justify-content: flex-end;
  margin-top: 25rpx;
}

.btn{
  border: 1px solid gray;
  display: inline-block;
  float: right;
  padding: 10rpx 20rpx;
  border-radius: 50rpx;
}