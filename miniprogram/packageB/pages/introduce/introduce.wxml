<!--pages/service/introduce/introduce.wxml-->
<view class="title">
  美食荟萃
</view>

<view wx:for='{{listData}}' wx:key='{{index}}'data-data="{{item}}">
  <view class="content">{{item.content}}</view>
  <view>{{item.icon}}</view>
  <view class="imgDiv">
    <image src='{{item.src}}' style='width:100%;'></image>
  </view>
</view>
