      <image class="userinfo-avatar head_img" src="{{userInfo.avatarUrl?userInfo.avatarUrl:'../../images/head_img.png'}}" mode="cover"></image>
    </view>
  </view>

  <navigator url="../../packageB/pages/bookingR" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
