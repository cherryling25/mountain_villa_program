<!--pages/discover/discover.wxml-->
<!--轮播-->
<view class="weui-padding-wrap">
  <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
    <block wx:for="{{imgUrls}}" wx:key="index" class="img_content">
      <swiper-item>
        <image src="{{item}}" class="slide-image" class="img" />
      </swiper-item>
    </block>
  </swiper>
</view>

<!--图标-->
<view class="iconFlex">
  <view>
    <navigator url="../../packageB/pages/orderFood/orderFood">
    <view><image src="../../images/order_food.png" class="flex_img"></image></view>
    <view style="font-size:28rpx;">点餐</view>
    </navigator>
  </view>

  <view>
    <navigator url="../../packageB/pages/orderRoom/roomDetails/roomDetails">
    <view><image src="../../images/order_room.png" class="flex_img"></image></view>
    <view style="font-size:28rpx;">订房</view>
    </navigator>
  </view>
</view>


<!--故事集-->
<view wx:for="{{stories}}" wx:key="index">
  <view class="title">{{item.title}}</view>
  <view class="weui-flex">
    <view class="weui-flex__item">
      <view class="content">
      <navigator url="{{item.url}}">
        <image src="{{item.src}}" class="flex1_img"></image>
        </navigator>
      </view>
    </view>
  </view>
</view>

